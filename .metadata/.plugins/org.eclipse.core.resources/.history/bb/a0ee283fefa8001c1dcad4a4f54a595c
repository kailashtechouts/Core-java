    Make sure to have dynamic inputs.*/
 class Operation {
	
	final double plus = 10;
	final double minus = 20;
	final double divide = 30;
	final double multiple = 40;
	
	public void operate(double plus, double minus, double divide, double multiple) {
		return ;
		
		void plus(double a, double b) {
			System.out.println(a + b);
		}
	}

}
