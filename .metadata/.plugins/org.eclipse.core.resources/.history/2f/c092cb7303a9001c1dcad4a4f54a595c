package org.assign2.java;

public class Task2 {

	public static void main(String[] args) {

		Teacher t1 = new Teacher("Rajarajeshwari", "Biology");
		Teacher t2 = new Teacher("Geeta", "All");
		Student s1 = new Student(101,"Indrajit", 99);
		Student s2 = new Student(102, "Chetan", 91);
		System.out.println("Teacher has to students:"+s1.name+"->"+s2.name);
		System.out.println("Student has to Teachers:" +t1.sub+"->"+t2.sub);
	}

}
